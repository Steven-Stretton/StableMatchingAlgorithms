<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<BorderPane prefHeight="500.0" prefWidth="1000.0" style="-fx-background-color: #0B0C10;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="uk.ac.bham.Main">
   <top>
      <VBox style="-fx-alignment: center;" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets bottom="25.0" left="25.0" right="25.0" top="25.0" />
         </BorderPane.margin>
         <children>
            <HBox alignment="TOP_RIGHT" VBox.vgrow="ALWAYS">
               <children>
                  <Button fx:id="LandingPageInfo" mnemonicParsing="false" onAction="#LandingPageInfo_run" style="-fx-background-color: #1F2833 #1F2833; -fx-border-color: #66fcf1 #66fcf1;" text="Info" textFill="#c5c6c7" HBox.hgrow="ALWAYS">
                     <font>
                        <Font size="17.0" />
                     </font>
                  </Button>
               </children>
            </HBox>
            <Text fill="#66fcf1" strokeType="OUTSIDE" strokeWidth="0.0" text="Stable-Matching Algorithms" textAlignment="CENTER" VBox.vgrow="ALWAYS">
               <font>
                  <Font name="Impact" size="60.0" />
               </font>
            </Text>
         </children>
      </VBox>
   </top>
   <center>
      <VBox BorderPane.alignment="CENTER">
         <children>
            <HBox alignment="BOTTOM_CENTER" style="-fx-spacing: 5; -fx-alignment: top-center;" VBox.vgrow="ALWAYS">
               <children>
                  <VBox alignment="CENTER" style="-fx-spacing: 10;" HBox.hgrow="ALWAYS">
                     <children>
                        <Text fill="#c5c6c7" strokeType="OUTSIDE" strokeWidth="0.0" text="Stable-roommates based on personality types" textAlignment="CENTER" wrappingWidth="285.0">
                           <font>
                              <Font size="20.0" />
                           </font>
                        </Text>
                        <Button fx:id="MBTI_Btn" mnemonicParsing="false" onAction="#MBTI_run" prefWidth="250.0" style="-fx-background-color: #1F2833; -fx-border-color: #66fcf1;" text="M.B.T.I Implementation" textAlignment="CENTER" textFill="#c5c6c7" wrapText="true">
                           <font>
                              <Font size="20.0" />
                           </font>
                        </Button>
                     </children>
                  </VBox>
                  <VBox alignment="CENTER" style="-fx-spacing: 10;" HBox.hgrow="ALWAYS">
                     <children>
                        <Text fill="#c5c6c7" strokeType="OUTSIDE" strokeWidth="0.0" text="Stable-roommates based on given preferences" textAlignment="CENTER" wrappingWidth="285.0">
                           <font>
                              <Font size="20.0" />
                           </font>
                        </Text>
                        <Button fx:id="EnterPref_Btn" mnemonicParsing="false" onAction="#EnterPref_run" prefWidth="250.0" style="-fx-background-color: #1F2833; -fx-border-color: #66fcf1;" text="Enter Preferences" textAlignment="CENTER" textFill="#c5c6c7">
                           <font>
                              <Font size="20.0" />
                           </font>
                        </Button>
                     </children>
                  </VBox>
                  <VBox alignment="CENTER" style="-fx-spacing: 10;" HBox.hgrow="ALWAYS">
                     <children>
                        <Text fill="#c5c6c7" strokeType="OUTSIDE" strokeWidth="0.0" text="Stable-roommates based on random preferences" textAlignment="CENTER" wrappingWidth="285.0">
                           <font>
                              <Font size="20.0" />
                           </font>
                        </Text>
                        <Button fx:id="RandomPref_Btn" mnemonicParsing="false" onAction="#RandomPref_run" prefWidth="250.0" style="-fx-background-color: #1F2833; -fx-border-color: #66fcf1;" text="Random Preferences" textAlignment="CENTER" textFill="#c5c6c7">
                           <font>
                              <Font size="20.0" />
                           </font>
                        </Button>
                     </children>
                  </VBox>
               </children>
               <padding>
                  <Insets left="50.0" right="50.0" top="50.0" />
               </padding>
            </HBox>
            <VBox prefHeight="312.0" prefWidth="1000.0" VBox.vgrow="ALWAYS">
               <VBox.margin>
                  <Insets />
               </VBox.margin>
               <children>
                  <HBox prefHeight="100.0" prefWidth="200.0" style="-fx-alignment: center;">
                     <children>
                        <VBox alignment="CENTER" style="-fx-spacing: 10;">
                           <HBox.margin>
                              <Insets bottom="50.0" top="50.0" />
                           </HBox.margin>
                           <children>
                              <Text fill="#c5c6c7" strokeType="OUTSIDE" strokeWidth="0.0" text="Stable-marraige based on programming experience" textAlignment="CENTER" wrappingWidth="285.0">
                                 <font>
                                    <Font size="20.0" />
                                 </font>
                              </Text>
                              <Button fx:id="StableMarriage_Btn" contentDisplay="CENTER" mnemonicParsing="false" onAction="#StableMarriage_run" prefWidth="250.0" style="-fx-background-color: #1F2833; -fx-border-color: #66fcf1;" text="Enter Experience" textAlignment="CENTER" textFill="#c5c6c7">
                                 <font>
                                    <Font size="20.0" />
                                 </font>
                              </Button>
                           </children>
                        </VBox>
                     </children>
                  </HBox>
               </children>
            </VBox>
         </children>
      </VBox>
   </center>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
</BorderPane>
